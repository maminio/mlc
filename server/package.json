{
    "name": "mlc-server",
    "version": "1.0.1",
    "private": true,
    "standard": {
        "env": [
            "mocha"
        ]
    },
    "scripts": {
        "flow": "flow",
        "cron-catalog": "nodemon --exec babel-node ./bin/www",
        "dev": "nodemon --exec babel-node ./bin/www",
        "debug": "nodemon --watch src lib public ../shared --exec \"babel-node ./bin/www\" --ignore public/js",
        "start": "nodemon --watch src lib public ../shared --exec \"babel-node ./bin/www\" --ignore ./src/routes/inference/function --ignore ./src/services/builder/model",
        "inference": "SERVICE_NAME='inference' nodemon --watch src lib public ../shared --exec \"babel-node ./bin/www\" --ignore ./src/routes/inference/function --ignore ./src/services/builder/model",
        "start-prod": "babel-node ./bin/www",
        "test": "NODE_ENV=testing mocha --require @babel/register --require @babel/polyfill \"src/**/*.js\" \"src-test/**/*.js\" --slow 25 --timeout 1500 --check-leaks",
        "ci-test": "yarn run test"
    },
